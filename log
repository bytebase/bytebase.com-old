  {
    "objectID": "/vcs-integration/name-and-organize-schema-files322",
    "url": "/docs/vcs-integration/name-and-organize-schema-files",
    "hierarchy": {
      "lvl0": "Documentation",
      "lvl1": "What is Bytebase",
      "lvl2": "Key Features",
      "lvl3": "Tenant Database Management",
      "lvl4": "Supported wildcard",
      "lvl5": null,
      "lvl6": null
    },
    "type": "content",
    "content": "\n\nUse '*' to match one directory. For example:\n\n{{ENV_NAME}}/*/{{DB_NAME}}##{{VERSION}}##{{TYPE}}##{{DESCRIPTION}}.sql matches env1/foo/db1##202101131000##ddl##create_tablefoo_for_bar.sql.\n\nUse '**' to match one or more directories. For example:\n\n{{ENV_NAME}}/**/{{DB_NAME}}##{{VERSION}}##{{TYPE}}##{{DESCRIPTION}}.sql matches env1/foo/bar/db1##202101131000##ddl##create_tablefoo_for_bar.sql\n"
  },
