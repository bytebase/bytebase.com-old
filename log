    "objectID": "/vcs-integration/troubleshoot401",
    "url": "/docs/vcs-integration/troubleshoot#potential-solution",
    "hierarchy": {
      "lvl0": "Documentation",
      "lvl1": "Data Model",
      "lvl2": "Issue",
      "lvl3": "OAuth CORS error with old GitLab version",
